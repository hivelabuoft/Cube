	"A comet chart showing changes between between two states": {
		"url": "/vega-lite/examples/trail_comet.html",
		"spec": {
			"$schema": "https://vega.github.io/schema/vega-lite/v5.json",
			"data": {
				"url": "data/barley.json"
			},
			"title": {
				"text": "Barley Yield comparison between 1932 and 1931",
				"fontSize": 22
			},
			"width": 80,
			"height": 350,
			"transform": [{
				"pivot": "year",
				"value": "yield",
				"groupby": ["variety", "site"]
			}, {
				"fold": ["1931", "1932"],
				"as": ["year", "yield"]
			}, {
				"calculate": "toNumber(datum.year)",
				"as": "year"
			}, {
				"calculate": "datum['1932'] - datum['1931']",
				"as": "delta"
			}],
			"mark": "trail",
			"encoding": {
				"x": {
					"field": "year",
					"axis": {
						"title": null,
						"labelFontSize": 14,
						"titleFontSize": 20
					}
				},
				"y": {
					"field": "variety",
					"axis": {
						"title": "Variety",
						"labelFontSize": 14,
						"titleFontSize": 20
					}
				},
				"size": {
					"field": "yield",
					"type": "quantitative",
					"scale": {
						"range": [0, 12]
					},
					"legend": {
						"values": [20, 60],
						"title": "Barley Yield (bushels/acre)",
						"titleFontSize": 13,
						"labelFontSize": 12
					}
				},
				"tooltip": [{
					"field": "year",
					"type": "quantitative"
				}, {
					"field": "yield"
				}],
				"color": {
					"field": "delta",
					"type": "quantitative",
					"scale": {
						"domainMid": 0
					},
					"legend": {
						"title": "Yield Delta (%)",
						"titleFontSize": 13,
						"labelFontSize": 12
					}
				},
				"column": {
					"field": "site",
					"title": "Site"
				}
			},
			"view": {
