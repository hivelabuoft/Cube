					"type": "rule",
					"color": "#ccc"
				},
				"encoding": {
					"detail": {
						"aggregate": "count"
					},
					"x": {
						"field": "key"
					}
				}
			}, {
				"mark": "line",
				"encoding": {
					"color": {
						"type": "nominal",
						"field": "Species",
