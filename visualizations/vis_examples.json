					},
					"x": {
						"type": "nominal",
						"field": "key"
					},
					"y": {
						"type": "quantitative",
						"field": "norm_val",
						"axis": null
					},
					"tooltip": [{
						"type": "quantitative",
						"field": "Beak Length (mm)"
					}, {
						"type": "quantitative",
						"field": "Beak Depth (mm)"
					}, {
						"type": "quantitative",
						"field": "Flipper Length (mm)"
					}, {
						"type": "quantitative",
						"field": "Body Mass (g)"
					}]
				}
			}, {
				"encoding": {
					"x": {
						"type": "nominal",
						"field": "key"
					},
					"y": {
						"value": 0
					}
				},
				"layer": [{
					"mark": {
						"type": "text",
						"style": "label",
						"fontSize": 15
					},
					"encoding": {
						"text": {
							"aggregate": "max",
							"field": "max"
						}
					}
				}, {
					"mark": {
						"type": "tick",
						"style": "tick",
						"size": 8,
						"color": "#ccc"
					}
				}]
			}, {
				"encoding": {
					"x": {
						"type": "nominal",
						"field": "key"
					},
					"y": {
						"value": 150
					}
				},
				"layer": [{
					"mark": {
						"type": "text",
						"style": "label",
						"fontSize": 15
					},
					"encoding": {
						"text": {
							"aggregate": "min",
							"field": "mid"
						}
					}
				}, {
					"mark": {
						"type": "tick",
						"style": "tick",
						"size": 8,
						"color": "#ccc"
					}
				}]
			}, {
				"encoding": {
					"x": {
						"type": "nominal",
