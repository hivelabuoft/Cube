	"Stacked Bar Chart": {
		"url": "/vega-lite/examples/stacked_bar_weather.html",
		"spec": {
			"$schema": "https://vega.github.io/schema/vega-lite/v5.json",
			"title": {
				"text": "Number of Days per Weather Type from 2020 to 2023",
				"fontSize": 22
			},
			"width": 750,
			"height": 500,
			"data": {
				"url": "data/seattle-weather.csv"
